import java.util.*;
//프로그래머스 레벨2 '점프와 순간이동'
public class Solution {
    public int solution(int n) {
        int ans = 0;
        while(n != 0){
            if(n % 2 == 0){
                n /=2;
            }else{
                n--;
                ans++;
            }
        }
        return ans;
    }
}
